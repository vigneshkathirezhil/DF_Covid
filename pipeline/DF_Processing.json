{
	"name": "DF_Processing",
	"properties": {
		"activities": [
			{
				"name": "LookUp_GetListFileToDownload",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"dataset": {
						"referenceName": "ReadJsonFileToDownload",
						"type": "DatasetReference"
					}
				}
			},
			{
				"name": "IterateTheFileDownload",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "LookUp_GetListFileToDownload",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('LookUp_GetListFileToDownload').output",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "SetForEachValues",
							"type": "SetVariable",
							"dependsOn": [],
							"userProperties": [],
							"typeProperties": {
								"variableName": "BaseUrl",
								"value": {
									"value": "@item().sourceBaseURL",
									"type": "Expression"
								}
							}
						}
					]
				}
			}
		],
		"parameters": {
			"sourceBaseURL": {
				"type": "string"
			},
			"sourceRelativeURL": {
				"type": "string"
			},
			"sinkFileName": {
				"type": "string"
			}
		},
		"variables": {
			"BaseUrl": {
				"type": "String"
			}
		},
		"annotations": []
	}
}