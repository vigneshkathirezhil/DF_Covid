{
	"name": "flatfiledest",
	"properties": {
		"linkedServiceName": {
			"referenceName": "movethedata",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"flatfilename": {
				"type": "String",
				"defaultValue": "flatparameter.csv"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().flatfilename",
					"type": "Expression"
				},
				"fileSystem": "temp"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "country",
				"type": "String"
			},
			{
				"name": "country_code_2_digit",
				"type": "String"
			},
			{
				"name": "country_code_3_digit",
				"type": "String"
			},
			{
				"name": "continent",
				"type": "String"
			},
			{
				"name": "population",
				"type": "String"
			}
		]
	}
}